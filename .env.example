# NATS Configuration
NATS_URL=nats://nats.nats.svc.cluster.local:4222
INPUT_STREAM=bluesky-posts
OUTPUT_STREAM=bluesky-posts-enriched
INPUT_SUBJECT=bluesky.posts
OUTPUT_SUBJECT=bluesky.enriched
CONSUMER_NAME=unified-processor
QUEUE_GROUP=unified-processor
NUM_STREAM_REPLICAS=1

# Service Configuration
SERVICE_NAME=nats-stream-processor
ACK_WAIT_SECONDS=30
MAX_DELIVER=3
MAX_ACK_PENDING=100
DUPLICATE_WINDOW_SECONDS=600

# Sentiment Model Configuration
SENTIMENT_MODEL_NAME=onnx-community/twitter-roberta-base-sentiment-ONNX
SENTIMENT_MODEL_CACHE_DIR=./models/sentiment
SENTIMENT_MAX_SEQUENCE_LENGTH=512
SENTIMENT_CONFIDENCE_THRESHOLD=0.4

# Topic Model Configuration
TOPIC_MODEL_NAME=richardr1126/tweet-topic-21-multi-ONNX
TOPIC_MODEL_CACHE_DIR=./models/topics
TOPIC_MAX_SEQUENCE_LENGTH=512
TOPIC_SIGMOID_THRESHOLD=0.5

# Performance Configuration
MAX_RETRIES=3
RETRY_DELAY=1.0

# Health Check Configuration
HEALTH_CHECK_PORT=8080
METRICS_ENABLED=true

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=console

# Keep this new line at the end of the file
