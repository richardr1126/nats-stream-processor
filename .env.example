# NATS Configuration
NATS_URL=nats://nats.nats.svc.cluster.local:4222
INPUT_STREAM=bluesky-posts
OUTPUT_STREAM=bluesky-posts-sentiment
INPUT_SUBJECT=bluesky.posts
OUTPUT_SUBJECT=bluesky.posts.sentiment
CONSUMER_NAME=sentiment-processor
QUEUE_GROUP=sentiment-processor
NUM_STREAM_REPLICAS=1

# Service Configuration
SERVICE_NAME=nats-stream-processor
PROCESSING_TIMEOUT=30

# Sentiment Model Configuration
MODEL_NAME=onnx-community/twitter-roberta-base-sentiment-ONNX
MODEL_CACHE_DIR=./models
MAX_SEQUENCE_LENGTH=512
CONFIDENCE_THRESHOLD=0.4

# Performance Configuration
MAX_RETRIES=3
RETRY_DELAY=1.0

# Health Check Configuration
HEALTH_CHECK_PORT=8080
METRICS_ENABLED=true

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=console

# Keep this new line at the end of the file
